<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="principal\principalBundle\Entity\Empresas" table="empresas">
    <id name="idempresas" type="integer" column="idempresas">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="text" column="nombre" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="rif" type="string" column="rif" length="9" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telefono" type="string" column="telefono" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="representante" type="string" column="representante" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
